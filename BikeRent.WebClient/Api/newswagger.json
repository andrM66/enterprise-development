  "components": {
    "schemas": {
      "Bike": {
        "required": [
          "color",
          "id",
          "model",
          "typeId"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "typeId": {
            "type": "integer",
            "format": "int32"
          },
          "model": {
            "maxLength": 10,
            "minLength": 1,
            "type": "string"
          },
          "color": {
            "maxLength": 10,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "BikeDto": {
        "required": [
          "color",
          "model",
          "typeId"
        ],
        "type": "object",
        "properties": {
          "typeId": {
            "type": "integer",
            "description": "Bike's type's id",
            "format": "int32"
          },
          "model": {
            "type": "string",
            "description": "Bike's model",
            "nullable": true
          },
          "color": {
            "type": "string",
            "description": "Bike's color",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "BikeType": {
        "required": [
          "id",
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "price": {
            "maxLength": 10,
            "type": "number",
            "format": "double"
          },
          "name": {
            "maxLength": 10,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "BikeTypeDto": {
        "required": [
          "name",
          "price"
        ],
        "type": "object",
        "properties": {
          "price": {
            "type": "number",
            "description": "Rental cost",
            "format": "double"
          },
          "name": {
            "type": "string",
            "description": "Type's name",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "Client": {
        "required": [
          "birthDate",
          "firstName",
          "id",
          "patronymic",
          "phoneNumber",
          "secondName"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "firstName": {
            "minLength": 1,
            "type": "string"
          },
          "secondName": {
            "minLength": 1,
            "type": "string"
          },
          "patronymic": {
            "minLength": 1,
            "type": "string"
          },
          "birthDate": {
            "type": "string",
            "format": "date-time"
          },
          "phoneNumber": {
            "maxLength": 12,
            "minLength": 1,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "ClientDto": {
        "required": [
          "birthDate",
          "firstName",
          "patronymic",
          "phoneNumber",
          "secondName"
        ],
        "type": "object",
        "properties": {
          "firstName": {
            "type": "string",
            "description": "Client's first name",
            "nullable": true
          },
          "secondName": {
            "type": "string",
            "description": "Client's second name",
            "nullable": true
          },
